    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>539f2c1a-8886-472f-a614-2e31e1f02c17</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.6" />
